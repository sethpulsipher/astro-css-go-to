{
  "name": "astro-css-go-to",
  "displayName": "Astro CSS Go To",
  "description": "Jump to CSS class definitions in Astro files.",
  "version": "0.0.1",
  "publisher": "sethify",
  "repository": "https://github.com/sethpulsipher/astro-css-go-to",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:astro"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "astro-css-go-to.goToDefinition",
        "title": "Astro: Go to CSS Definition"
      }
    ],
    "keybindings": [
      {
        "command": "astro-css-go-to.goToDefinition",
        "key": "F12",
        "when": "editorTextFocus && editorLangId == 'astro'"
      }
    ],
    "configuration": {
      "title": "Astro CSS Go To",
      "properties": {
        "astro-css-go-to.definitionAction": {
          "type": "string",
          "default": "jump",
          "enum": [
            "jump",
            "peek"
          ],
          "description": "Specifies the action to take when going to a CSS definition.",
          "enumDescriptions": [
            "Jump directly to the CSS definition.",
            "Show the CSS definition in a peek editor."
          ]
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}